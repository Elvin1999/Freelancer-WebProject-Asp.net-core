@model IEnumerable<FreelancerOwnPostVM>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
@foreach (var item in Model)
{

    <div class="post-bar">
        <div class="post_topbar">
            <div class="usy-dt">
                <img src="~/ClientView/images/resources/us-pic.png" alt="">
                <div class="usy-name">
                    <h3>@item.PostName</h3>
                    <span><img src="images/clock.png" alt="">3 min ago</span>
                </div>
            </div>     
        </div>        
        <div class="job_descp">
            <h3>@item.PostName</h3>
            <ul class="job-dt">
                <li><span>$@item.PostPrice</span></li>
            </ul>
            <p>@item.PostDescription <a href="#" title="">view more</a></p>
            <ul class="skill-tags">
                <li><a href="#" title="">@item.PostDeadLine.ToLongDateString()</a></li>

            </ul>
        </div>
        <div class="job-status-bar">
            <ul class="like-com">
                <li>
                    <a href="#" class="active"><i class="fas fa-heart"></i> Like</a>
                    <img src="images/liked-img.png" alt="">
                    <span>25</span>
                </li>
                <li><a href="#" class="com"><i class="fas fa-comment-alt"></i> Bids 2</a></li>
            </ul>
            <a href="#"><i class="fas fa-eye"></i>Views 50</a>
        </div>
    </div>
}